#include <stdio.h>

int main()
{
   float a, b, c, d, e, total;
   
   printf("enter the value of a :");
   scanf("%f",&a);
   printf("enter the value of b :");
   scanf("%f",&b);
   printf("enter the vslue of c :");
   scanf("%f",&c);
   printf("enter the value of d :");
   scanf("%f",&d);
   printf("enter the value of e :");
   scanf("%f",&e);
   
   total=a+b+c+d+e;
   
   printf("sum of total numbers : %f", total);
   
   return 0;
}

