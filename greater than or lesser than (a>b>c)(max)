#include <stdio.h>

int main()
{
    int a,b,c;
    printf("enter the value a :");
    scanf("%d",&a);
    printf("the enter the value of b:");
    scanf("%d",&b);
    printf("enter the value c :");
    scanf("%d",&c);

    if(a>b &&a>c)
    {
        printf("a is max");
    }
    else if(b>a&&b>c)
    {
        printf("b is max");
    }
    else
    {
        printf("c is max");
    }
   return 0;
}




