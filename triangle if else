#include <stdio.h>

int main()
{
    int a,b,c;
     printf("enter the 3 values of the triangle:");
    scanf("%d %d %d",&a,&b,&c);

    if(a==b && a==c && c==a)
    {
        printf("it is equilateral");
    }
    else if(a==b || a==c || c==a)
    {
        printf("it's isoceles ");
    }
    else 
    {
        printf("it's scalen");
    }
  

    return 0;
}
