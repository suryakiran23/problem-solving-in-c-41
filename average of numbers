#include <stdio.h>

int main()
{
   float a, b, c, d, e, average;
   
   printf("enter the value of a :");
   scanf("%f",&a);
   printf("enter the value of b :");
   scanf("%f",&b);
   printf("enter the vslue of c :");
   scanf("%f",&c);
   printf("enter the value of d :");
   scanf("%f",&d);
   printf("enter the value of e :");
   scanf("%f",&e);
   
  average=(a+b+c+d+e)/5;
   
   
   printf("average of 5 subject marks :%f ", average);
   
   return 0;
}



